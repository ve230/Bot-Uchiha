const { xp, nivel } = require("./xp"); // Se vocÃª salvou xp.js como mÃ³dulo

module.exports = {
    name: "rankingxp",
    execute: async (sock, msg) => {
        const jid = msg.key.remoteJid;
        const ranking = Object.entries(xp)
            .sort((a,b) => b[1]-a[1])
            .slice(0,10)
            .map((u,i) => `${i+1}. ${u[0]} - XP: ${u[1]}, NÃ­vel: ${nivel[u[0]] || 1}`)
            .join("\n");

        await sock.sendMessage(jid, { text: `ğŸ† Ranking de XP:\n${ranking}` });
    }
};