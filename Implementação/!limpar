module.exports = {
    name: "limpar",
    execute: async (sock, msg, args) => {
        const jid = msg.key.remoteJid;
        if (!config.vipUsers.includes(msg.key.participant || jid)) return sock.sendMessage(jid, { text: "âŒ Acesso negado!" });
        await sock.sendMessage(jid, { delete: { remoteJid: jid, fromMe: true } });
    }
};