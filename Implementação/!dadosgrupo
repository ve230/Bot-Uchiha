module.exports = {
    name: "dadosgrupo",
    execute: async (sock, msg) => {
        const jid = msg.key.remoteJid;
        try {
            const metadata = await sock.groupMetadata(jid);
            const info = `ğŸ“Œ Nome: ${metadata.subject}\nğŸ‘¥ Participantes: ${metadata.participants.length}\nğŸ“ Desc: ${metadata.desc?.toString() || "Sem descriÃ§Ã£o"}`;
            await sock.sendMessage(jid, { text: info });
        } catch {
            await sock.sendMessage(jid, { text: "âŒ Falha ao obter informaÃ§Ãµes." });
        }
    }
};