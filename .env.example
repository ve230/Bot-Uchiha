# Bot Configuration Environment Variables

# API Keys
API_KEY="your_api_key_here"
DATABASE_URL="your_database_url_here"

# Database Settings
DB_USER="your_db_user_here"
DB_PASSWORD="your_db_password_here"
DB_HOST="localhost"
DB_PORT="5432"  # or your specific port

# Bot Owner Information
BOT_OWNER_ID="your_bot_owner_id_here"
BOT_OWNER_EMAIL="your_owner_email_here"

# Premium Settings
PREMIUM_FEATURE_ENABLED="false"
PREMIUM_API_KEY="your_premium_api_key_here"

# Additional Configuration
LOG_LEVEL="info"  # Log level can be "debug", "info", "warn", "error"